import iotDevice

external mean
external log
 
connectionConfig Wifi{
	type:WLAN
	"ssid":"Network2GHz"
	"password": "kalenderlys"
	
}

iot device Cup {
	setup Wifi
	list temps
	
	every 10 MILLISECONDS{
		read from TEMPERATURE -> add to temps
	}
	
	every 100 MILLISECONDS {
		mean of temps ->  send to Server
		clear temps
	}
	
}


controller device Server{
	
	listen on 192.168.0.16:8000 -> log of value
	
}